{
  "manifest_version": 2,
  "name": "Send It",
  "description": "Send pupper pics to mein wife.",
  "version": "1.5",
  "homepage_url": "https://github.com/sselfridge/send-it",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["activeTab", "contextMenus"]
}
